    <section class="section-take">
        <div class="container-fluid">
            <div class="flex-center h-75">
                <div class="signin-header">
                    Quiz Score:
                </div>
            </div>
            <div class="card mx-auto h-100">
                <div class="card-body">

                    <div class="text-center">
                        <div class="font">Quiz Name:</div>
                        <div class="result-font">{{Name}}</div><br>
                        <div class="font">Quiz Score:</div>
                        <div class="result-font">{{Score}}</div>
                    </div>

                </div>
            </div>
        </div>
    </section>